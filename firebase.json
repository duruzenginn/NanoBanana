{
	"functions": {
		"source": "functions"
	},
	"emulators": {
		"functions": {
			"host": "127.0.0.1",
			"port": 5002
		}
	},
		"hosting": {
			"public": "frontend/dist",
			"predeploy": [
				"npm --prefix frontend run build"
			],
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [
					{
						"source": "/api/generateImage",
						"function": {
						"functionId": "generateImageV2",
							"region": "europe-west1"
						}
					},
					{
						"source": "/api/freepik/search",
						"function": {
							"functionId": "freepikSearch",
							"region": "europe-west1"
						}
					},
					{
						"source": "/api/hello",
						"function": {
							"functionId": "genkitHello",
							"region": "europe-west1"
						}
					}
		]
	}
}
